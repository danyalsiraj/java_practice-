public class strings {
	public static void main (String[] args)
	{
		//int n[]= new int [5];
		int n[]={1,3,4,5,9};
		int temp[]=new int [n.length];
		int i,j;
		
		for (j=0;j<n.length;j++)
		{
		
		for (i=0;i<n.length;i++)
		{
			if (n[i]<n[j]){
				n[j]=n[i];
				
				
			}
		}
			
		}
		System.out.println(n);
		
	}
	
	
	
}
